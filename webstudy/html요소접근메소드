//접근메소드 -	document.getElementsById('class이름')
	 		document.getElementsTagName('tag이름')
	 	    document.getElementsByName('name이름')
	 		     
	 		document.formname.요소name
	 		//결과값 배열x 단수o
	 		document.querySelector("#이름")
	 		document.querySelector(".이름")
	 		document.querySelector("tag이름")
	 		     
	 		document.querySelectorAll('.이름')
	 		document.querySelectorAll('tag이름');
출력
innerHTML = str;
출력하는 str속에 html태그가 포함 되었을 경우

innerText = str;
출력하는 str속에 html태그가 포함 되어 있지 않은 경우